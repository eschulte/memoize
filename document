#!/usr/bin/sbcl --script
;; -*- lisp -*-
(load "~/.sbclrc" :if-does-not-exist nil)
(require :memoize)
(require :schulte-documentation-template)
(schulte-documentation-template:create-template
 :memoize
 :target "memoize.html"
 :subtitle "thread safe function memoization"
 :abstract "A tiny memoization library built on FLEXI-STREAMS and
    CL-STORE. This uses implementation-specific (currently supports
    SBLC and Clozure CL) thread safe hashes to ensure thread safe
    memoization."
 :source "http://github.com/eschulte/memoize")
